          }
        }
      ]
    },
    {
      "**************************************************": "OUTPUT VARIANTS"
    },
    {
      "name": "result",
      "explanation": "Verbosity of the resultPattern output.",
      "entries": [
        "{summary}",
        "{detail}"
      ]
    },
    {
      "name": "word",
      "explanation": "One-word output.",
      "entries": [
        "{location}"
      ]
    },
    {
      "name": "summary",
      "explanation": "Brief resultPattern output.",
      "entries": [
        "{attribute} {location}"
      ]
    },
    {
      "name": "detail",
      "explanation": "Detailed resultPattern output variations.",
      "entries": [
        "{location}, {complication}",
        "{attribute} {location}, {complication}"
      ]
    },
    {
      "name": "description",
      "explanation": "Long description as a sentence.",
      "entries": [
        "The {location} {synonym} {looks} {feature}.",
        "The {attribute} {location} {synonym} {looks} {feature}."
      ]
    },
    {
      "name": "section",
      "explanation": "Long description as a subheading and a text paragraph.",
      "entries": [
        "===== {random} =====\r\n{b|{meta}} \r\n "
      ]
    },
    {
      "name": "stats",
      "explanation": "Details as statistics.",
      "entries": [
        "{b|Name}: {random} \r\n{b|Attribute}: {attribute} \r\n{b|Feature}: {feature} "
      ]
    },
    {
      "name": "meta",
      "explanation": "Off-game explanation of the result.",
      "entries": [
        ""
