{
  "name": "name-pictish",
  "resultPattern": "{result}",
  "globals": {
    "culture": "Pictish"
  },
  "variables": {
    "footnote": "note"
  },
  "explanation": "Pict names.",
  "structure": "Genders are collected in the same file to allow easy sharing of patronym and byname tables. Subtables have been split by their initial letter to allow future support for allitteration and for example siblings sharing the same initial.",
  "note": "Includes multiple spelling variations of the same names. Several hypotectical, reconstructed names are included as well.",
  "to do": "",
  "see": "",
  "date": "2018-03-10",
  "authors": "ELF Vesala",
  "sources": "Eithni.com, PeiraeusPublicLibrary.com,  S-Gabriel.org, Wikipedia.org",
