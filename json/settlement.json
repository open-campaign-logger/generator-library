      "**************************************************": "OUTPUT VARIANTS"
    },
    {
      "name": "result",
      "explanation": "The output format.",
      "entries": [
        "{word}",
        "{summary}",
        "{detail}"
      ]
    },
    {
      "name": "word",
      "explanation": "One-word output.",
